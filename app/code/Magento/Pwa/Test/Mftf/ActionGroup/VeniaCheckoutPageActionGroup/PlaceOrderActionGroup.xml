<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">
    <actionGroup name="PlaceOrder">
        <waitForPwaElementVisible selector="{{VeniaCheckoutPageSection.placeOrder}}" stepKey="waitForPlaceOrderButtonToBeReady"/>
        <click selector="{{VeniaCheckoutPageSection.placeOrder}}" stepKey="clickOnPlaceOrderButton"/>
        <!--<wait time="15" stepKey="temporaryWait"/>-->
        <dontSeeElement selector="{{VeniaMiniCartIconSection.miniCartIcon}}" stepKey="assertMiniCartIconDoesNotExists"/>
    </actionGroup>
</actionGroups>
