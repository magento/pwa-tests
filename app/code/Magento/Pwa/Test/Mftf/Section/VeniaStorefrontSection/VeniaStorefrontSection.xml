<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="VeniaStorefrontSection">
        <element name="title" type="text" selector="[class*='categoryList-title-'] span"/>
        <element name="productImage" type="button" selector="[class*='categoryTile-image'][alt*='{{var1}}']" parameterized="true"/>
        <element name="categoryName" type="text" selector="//span[contains(text(), '{{categoryName}}')]" parameterized="true"/>
        <element name="filterButton" type="button" selector="button[class*='searchPage-filterButton-'] span[class*='button-content-']"/>
    </section>
</sections>
