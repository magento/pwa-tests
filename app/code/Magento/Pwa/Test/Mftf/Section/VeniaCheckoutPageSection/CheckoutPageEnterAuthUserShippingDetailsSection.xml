<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
<section name="CheckoutPageEnterAuthUserShippingDetailsSection">
    <element name="submitShippingInfo" type="button" selector="//span[contains(text(), 'Save and Continue')]"/>
    <element name="city" type="input" selector="//div[contains(@class, 'editModal-body-')]//input[contains(@name, 'city')]"/>
    <element name="selectState" type="select" selector="[class*='customerForm-region'] [class*='select-input-'][name*='region[region_id]']"/>
    <element name="zipCode" type="input" selector="//div[contains(@class, 'editModal-body-')]//input[contains(@name, 'postcode')]"/>
</section>
</sections>
