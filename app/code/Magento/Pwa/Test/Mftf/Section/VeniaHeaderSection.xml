<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="VeniaHeaderSection">
        <element name="hamburgerMenu" type="button" selector="[class*='navTrigger-root-']"/>
        <element name="logo" type="button" selector="[class*='header-toolbar-'] a"/>
        <element name="searchIcon" type="button" selector="[class*='searchTrigger-root-']"/>
        <element name="searchInput" type="input" selector="[class*='textInput-input-']"/>
        <element name="searchFieldClearData" type="button" selector="[class^='trigger-root-']"/>
        <element name="shoppingCartIcon" type="button" selector="[class*='cartTrigger-root-']"/>
    </section>
</sections>
